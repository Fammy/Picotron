--[[pod_format="raw",created="2024-03-27 01:47:01",modified="2024-04-13 17:59:44",revision=697]]
-- Pixel Rain v.2
-- by Fam

function _init()
	clear = 1
	x = -1
	y = -1
	lastY = -1
	step = 9
	msg = ""
	reset()
end

function reset()
	clearX = x
	clearY = lastY -1

	x = rnd(480)
	y = -1
	lastY = -1
	
	c = rnd(31) + 1
	if (c == bg) then c = 0 end
end

function _update()
	bg = theme("desktop0")

	lastY = y
	y += step
		
	if (y >= 270) then
		reset()
		return
	end
	
	if (pget(x, y) != bg) then
		local change = 0
		local tmp = y - step  + 1
		for newY = y - 1, y - step + 1, -1 do
			if (pget(x, newY) == bg) then
				y = newY
				change = 1
				break
			end
		end
		if (change == 0) then
			reset()
		end

		if (pget(x, 0) != bg) then
			clear = 1
		end
	end
end

function _draw()
	if (clear == 1) then
		cls(bg)
		clear = 0
	end
	
	if (clearX >= 0) then
		pset(clearX, clearY, bg)
		pset(clearX, clearY - 1, bg)
		clearX = -1
		clearY = -1
	end

	pset(x, y, c)
	pset(x, y - 1, c)
	pset(x, y - 2, c)

	pset(x, lastY, bg)
	pset(x, lastY - 1, bg)
	pset(x, lastY - 2, bg)
	
	if (msg != "") then
		rectfill(10, 250, 250, 270, bg)
		print(msg, 10, 250, 1)
	end
end
