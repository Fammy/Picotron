:: main.lua
--[[pod_format="raw",created="2024-03-25 00:12:45",modified="2024-04-24 00:19:41",revision=101]]
-- circle_wallpaper

function _init()
	x=rnd(480)
	y=rnd(240)
	c=1
	r=64
	tick=0
	
	stack = {}
end

function _update()
	tick+=1
	if (tick>=5) then tick=0 end

	c=rnd(31)+ 1
	r-=.1
	
	if (r<10) then
--		cls(0)
		x=rnd(480)
		y=rnd(240)
		c=1
		r=64
	end
end

function _draw()
	if (tick==0) then
		circfill(rnd(480),rnd(270),rnd(48)+16,rnd(31)+1)
	end
end
